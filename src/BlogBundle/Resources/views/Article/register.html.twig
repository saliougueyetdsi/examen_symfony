{% extends 'blog/layout.html.twig' %}
{% block body %}
<div class="well">
{{ form_start(form) }}
    {{ form_row(form.username) }}
    {{ form_row(form.email) }}
    {{ form_row(form.plainPassword.first) }}
    {{ form_row(form.plainPassword.second) }}

    <button type="submit">Register!</button>
{{ form_end(form) }}
</div>
{% endblock %}{% endblock %}
{% endblock %}